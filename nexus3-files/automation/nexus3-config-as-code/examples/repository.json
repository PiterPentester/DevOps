{
	"repositories": {
		"raw": {
			"hosted": [{
					"repoGO": {
						"blobstore": {
							"name": "storeGo",
							"strict_content_type_validation": "true"
						},
						"write_policy": "allow"
					}
				},
				{
					"repoFiles": {
						"blobstore": {
							"name": "storeFiles",
							"strict_content_type_validation": "true"
						},
						"write_policy": "allow"
					}
				}
			]

		},
		"docker": {
			"hosted": {
				"repoDocker": {
					"blobstore": {
						"name": "storeDocker",
						"strict_content_type_validation": "true"
					},
					"write_policy": "allow"
				}
			}
		}
	},
	"blobstores": {
		"file": [
			"storeYum",
			"storeGo",
			"storeFiles",
			"storeDocker"
		]
	}
}
